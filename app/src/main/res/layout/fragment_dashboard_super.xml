<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/log2"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".super_admin.dashboard_super">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView3"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_gravity="center"
            android:layout_margin="20dp"
            android:layout_marginTop="104dp"
            app:cardBackgroundColor="#FDFDFD"
            app:cardCornerRadius="22.5dp"
            app:cardElevation="10dp"
            app:layout_constraintEnd_toEndOf="@+id/viewpg"
            app:layout_constraintStart_toStartOf="@+id/addpg"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/nav_image"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_gravity="center|top"
                android:layout_marginTop="8dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/superadminprofile" />

            <TextView
                android:id="@+id/cardname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="35dp"
                android:background="@null"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="Personal"
                android:textColor="#090808" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/addpg"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_gravity="center"
            android:layout_margin="20dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="21dp"
            android:background="@drawable/gradient"
            app:cardBackgroundColor="#FDFDFD"
            app:cardCornerRadius="22.5dp"
            app:cardElevation="10dp"
            app:layout_constraintBottom_toTopOf="@+id/card_complaint"
            app:layout_constraintEnd_toStartOf="@+id/viewpg"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView3">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/add" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="80dp"
                android:background="@null"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="Add PG"
                android:textColor="#090808"
                android:textSize="15sp" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/viewpg"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_gravity="center"
            android:layout_margin="20dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="21dp"
            app:cardBackgroundColor="#FDFDFD"
            app:cardCornerRadius="22.5dp"
            app:cardElevation="10dp"
            app:layout_constraintBottom_toTopOf="@+id/additional"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/addpg"
            app:layout_constraintTop_toBottomOf="@+id/cardView3">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/viewpg1" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="80dp"
                android:background="@null"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="View PG"
                android:textColor="#090808"
                android:textSize="15sp" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card_complaint"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_gravity="center"
            android:layout_margin="20dp"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="10dp"
            app:cardBackgroundColor="#FDFDFD"
            app:cardCornerRadius="22.5dp"
            app:cardElevation="10dp"
            app:layout_constraintEnd_toStartOf="@+id/additional"
            app:layout_constraintHorizontal_bias="0.52"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/addpg">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="0dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/complaints" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="80dp"
                android:background="@null"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="Complaints"
                android:textColor="#090808"
                android:textSize="15sp" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/promotion"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="20dp"
            android:layout_marginBottom="40dp"
            app:cardBackgroundColor="#FDFDFD"
            app:cardCornerRadius="22.5dp"
            app:cardElevation="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/additional"
            app:layout_constraintHorizontal_bias="0.13"
            app:layout_constraintStart_toStartOf="@+id/card_complaint"
            app:layout_constraintTop_toBottomOf="@+id/additional"
            app:layout_constraintVertical_bias="1.0">

            <Button
                android:id="@+id/upload"
                android:layout_width="87dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:gravity="center"
                android:layout_marginStart="4dp"
                android:background="#BAFDFDFD"
                android:text="Upload Promotion Image" />

            <ImageView
                android:id="@+id/image"
                android:layout_width="58dp"
                android:layout_height="58dp"
                android:layout_gravity="center"
                android:layout_margin="20dp"
                android:scaleType="fitXY"
                android:src="@drawable/image1" />

            <Button
                android:id="@+id/load"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:layout_gravity="right|center_vertical"
                android:gravity="center"
                android:background="#BAFDFDFD"
                android:text="Send it" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/additional"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:layout_gravity="center"
            android:layout_margin="20dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="40dp"
            app:cardBackgroundColor="#FDFDFD"
            app:cardCornerRadius="22.5dp"
            app:cardElevation="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/card_complaint"
            app:layout_constraintTop_toBottomOf="@+id/viewpg">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical|center"
                android:src="@drawable/logout1" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="80dp"
                android:background="@null"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="Logout"
                android:textColor="#090808"
                android:textSize="15sp" />

        </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>
